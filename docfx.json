{
  "metadata": [
    {
      "src": [
        {
          "files": [ "*.sln", "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "RageCoop resource documentation",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "../docs",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}